# Configurações de API - Renomeie este arquivo para .env e adicione suas chaves

# OpenAI API Key
OPENAI_API_KEY=sua_chave_openai_aqui

# Google Gemini API Key (opcional)
GOOGLE_API_KEY=sua_chave_google_aqui

# ClickBank Credenciais
CLICKBANK_USERNAME=seu_usuario_clickbank
CLICKBANK_PASSWORD=sua_senha_clickbank
CLICKBANK_ACCOUNT_NICKNAME=seu_nickname_clickbank

# Hotmart Credenciais
HOTMART_EMAIL=seu_email_hotmart
HOTMART_PASSWORD=sua_senha_hotmart
HOTMART_CLIENT_ID=seu_client_id_hotmart
HOTMART_CLIENT_SECRET=seu_client_secret_hotmart

# Configurações do Streamlit (opcional)
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0

